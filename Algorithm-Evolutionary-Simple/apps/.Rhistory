nopar.1k <- read.table('nopar-1k.dat')
par2.512 <- read.table('par2-512.dat')
par4.512 <- read.table('par4-256.dat')
summary(nopar.1k)
summary(par2.512)
summary(par4.512)
boxplot( nopar.1k, par2.512, par4.512, ylab='Seconds')
boxplot( nopar.1k$V1, par2.512$V1, par4.512$V1, ylab='Seconds')
axis(1,labels=c('Secuencial','2', '4'))
axis(1,at=c(1:3),labels=c('Secuencial','2', '4'))
boxplot( nopar.1k$V1, par2.512$V1, par4.512$V1, ylab='Seconds',ylim=c(0,100), main='Tiempo medio')
axis(1,at=c(1:3),labels=c('Secuencial','2', '4'))
png('tiempos.png',width=800,height=800)
boxplot( nopar.1k$V1, par2.512$V1, par4.512$V1, ylab='Seconds',ylim=c(0,100), main='Tiempo medio')
axis(1,at=c(1:3),labels=c('Secuencial','2', '4'))
dev.off()
quit()
